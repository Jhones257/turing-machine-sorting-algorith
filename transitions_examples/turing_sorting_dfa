digraph TuringSorting {
	q0 [shape=circle]
	q1 [shape=circle]
	q2 [shape=circle]
	q3 [shape=circle]
	q4 [shape=circle]
	q5 [fillcolor=lightgray shape=doublecircle style=filled]
	q0 -> q1 [label="1,1,R"]
	q1 -> q1 [label="4,4,R"]
	q1 -> q2 [label="3,3,L"]
	q2 -> q2 [label="4,2,R"]
	q2 -> q1 [label="3,4,R"]
	q1 -> q2 [label="2,2,L"]
	q2 -> q1 [label="2,4,R"]
	q1 -> q3 [label="_,_,L"]
	q3 -> q0 [label="X,X,L"]
	q3 -> q5 [label="_,_,R"]
}
